[alias]
	f = fetch
	co = checkout --recurse-submodules
	cod = checkout -d --recurse-submodules
	br = branch
	bd = branch -D
	cam = commit -am
	caa = commit -a --amend --no-edit
	rbi = rebase -i
	st = status
	pl = pull
	poh = push origin HEAD
	stash-all = stash save --include-untracked
	graph = log --graph --all --decorate --stat --date=iso
	sh = stash
	shame = blame -w -M
	unstage = reset --soft HEAD^
	as = assembler

Host stash jump cjump jump+* cjump+* dev bmdev support kovert adnxscmdb actools
  StrictHostKeyChecking no

Host stash
  Hostname stash.corp.appnexus.com
  Port 7999
  User git

Host jump
  Hostname jump.adnxs.net

Host cjump
  Hostname 01.jump.nym2.appnexus.net
  Port 2022

Host jump+*
  ProxyCommand ssh jump nc $(echo %h | sed 's/^.*+//;/:/!s/$/ %p/;s/:/ /')

Host cjump+*
  ProxyCommand ssh cjump nc $(echo %h | sed 's/^.*+//;/:/!s/$/ %p/;s/:/ /')

Host dev bmdev
  ProxyCommand ssh jump nc %h %p
  RemoteForward 8022 localhost:22

Host dev
  Hostname 1042.rnguyen.user.nym2

Host bmdev
  Hostname 341.bm-rnguyen.user.nym2


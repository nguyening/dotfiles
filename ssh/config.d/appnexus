Host stash jump jump+* dev
  StrictHostKeyChecking no

Host stash
  Hostname stash.corp.appnexus.com
  Port 7999
  User git

Host jump
  Hostname jump.adnxs.net

Host jump+*
  ProxyCommand ssh jump nc $(echo %h | sed 's/^.*+//;/:/!s/$/ %p/;s/:/ /')

Host dev
  Hostname 540.bm-rnguyen.user.nym2
  ProxyCommand ssh jump nc %h %p
  RemoteForward 8022 localhost:22
